{
  "name": "express-ts-node-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "init": "yarn && cd client && yarn",
    "start": "concurrently --kill-others \"yarn run build && node ./dist/src/server\" \"cd client && yarn start\"",
    "dev": "concurrently --kill-others \"cd client && yarn start\" \"nodemon\"",
    "dev:server": "nodemon",
    "dev:client": "cd client && yarn start",
    "build": "rm -rf ./dist && tsc -p .",
    "clean": "rm -rf dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Nikolaos Kontakis",
  "license": "ISC",
  "dependencies": {
    "acorn": "^7.4.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.1.1",
    "minimist": "^1.2.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.8",
    "@types/helmet": "0.0.48",
    "@types/node": "^14.11.8",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.10.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.4",
    "ts-loader": "^8.4.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
